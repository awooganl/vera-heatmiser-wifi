      <name>SetRunMode</name>
      <argumentList>
        <argument>
          <name>FrostState</name>
          <direction>in</direction>
        </argument>
      </argumentList>
    </action>

	<action>
      <name>SetHoldTime</name>
      <argumentList>
        <argument>
          <name>HoldTime</name>
          <direction>in</direction>
        </argument>
      </argumentList>
    </action>
    
	<action>
      <name>SetHWMode</name>
      <argumentList>
        <argument>
          <name>NewHWMode</name>
          <direction>in</direction>
        </argument>
      </argumentList>
    </action>

  </actionList>
</scpd>
