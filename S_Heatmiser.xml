<?xml version="1.0"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
  <specVersion>
    <major>1</major>
    <minor>0</minor>
  </specVersion>
  
  <actionList>

	<action>
      <name>SetKeyLock</name>
      <argumentList>
        <argument>
          <name>KeyState</name>
          <direction>in</direction>
        </argument>
      </argumentList>
    </action>

	<action>
      <name>SetFrostTemp</name>
      <argumentList>
        <argument>
          <name>FrostTemp</name>
          <direction>in</direction>
        </argument>
      </argumentList>
    </action>
    
	<action>
      <name>SetCurrentSetpoint</name>
      <argumentList>
        <argument>
          <name>NewCurrentSetpoint</name>
          <direction>in</direction>
        </argument>
      </argumentList>
    </action>

	<action>
